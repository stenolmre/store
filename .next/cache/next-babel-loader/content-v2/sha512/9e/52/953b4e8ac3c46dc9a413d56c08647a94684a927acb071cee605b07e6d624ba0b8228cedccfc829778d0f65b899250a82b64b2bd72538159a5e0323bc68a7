\n  doSliding = (direction, position) => {\n    this.setState({\n      sliding: true,\n      direction,\n      position\n    })\n\n    setTimeout(() => {\n      this.setState({\n        sliding: false\n      })\n    }, 50)\n  }\n\n  nextSlide = () => {\n    const { position } = this.state\n    const { children } = this.props\n    const numItems = children.length\n\n    if (numItems === 2 && position === 1) return\n\n    this.doSliding(\"next\", position === numItems - 1 ? 0 : position + 1)\n  }\n\n  prevSlide = () => {\n    const { position } = this.state\n    const { children } = this.props\n    const numItems = children.length\n\n    if (numItems === 2 && position === 0) return\n\n    this.doSliding(\"prev\", position === 0 ? numItems - 1 : position - 1)\n  }\n\n  render() {\n    const { children } = this.props\n    const { sliding, direction, position } = this.state\n\n    const childrenWithProps = Children.map(children, child =>\n      cloneElement(child, {\n        numSlides: children.length || 1\n      })\n    )\n\n    return (\n      <div>\n        <Wrapper>\n          <CarouselContainer\n            sliding={sliding}\n            direction={direction}\n            numSlides={childrenWithProps.length}\n          >\n            {childrenWithProps.map((child, index) => (\n              <CarouselSlot\n                key={index}\n                order={this.getOrder(index)}\n                numSlides={childrenWithProps.length}\n                position={position}\n              >\n                {child}\n              </CarouselSlot>\n            ))}\n          </CarouselContainer>\n        </Wrapper>\n        <button onClick={() => this.prevSlide()}>Prev</button>\n        <button onClick={() => this.nextSlide()}>Next</button>\n      </div>\n    )\n  }\n}\n\nconst CarouselHooks = ({ children }) => {\n  const [sliding, setSliding] = useState(false)\n  const [direction, setDirection] = useState()\n  const [position, setPosition] = useState(0)\n\n  const childrenWithProps = Children.map(children, child =>\n    cloneElement(child, {\n      numSlides: children.length || 1\n    })\n  )\n\n  const getOrder = (itemIndex) => {\n    const numItems = children.length\n\n    if (numItems === 2) return itemIndex\n\n    if (itemIndex - position < 0)\n      return numItems - Math.abs(itemIndex - position)\n    return itemIndex - position\n  }\n\n  const doSliding = (direction, position) => {\n    setSliding(true)\n    setDirection(direction)\n    setPosition(position)\n\n    setTimeout(() => {\n      setSliding(false)\n    }, 50)\n  }\n\n  const nextSlide = () => {\n    const numItems = children.length\n\n    if (numItems === 2 && position === 1) return\n\n    doSliding(\"next\", position === numItems - 1 ? 0 : position + 1)\n  }\n\n  return (\n    <div>\n      <Wrapper>\n        <CarouselContainer\n          sliding={sliding}\n          direction={direction}\n          numSlides={childrenWithProps.length}\n        >\n          {childrenWithProps.map((child, index) => (\n            <CarouselSlot\n              key={index}\n              order={() => getOrder(index)}\n              numSlides={childrenWithProps.length}\n              position={position}\n            >\n              {child}\n            </CarouselSlot>\n          ))}\n        </CarouselContainer>\n      </Wrapper>\n      <button onClick={nextSlide}>Next</button>\n    </div>\n  )\n}\n\nconst Index = () => <div>\n  <Carousel>\n    <Item>Item 1</Item>\n    <Item>Item 2</Item>\n    <Item>Item 3</Item>\n    <Item>Item 4</Item>\n    <Item>Item 5</Item>\n    <Item>Item 6</Item>\n    <Item>Item 7</Item>\n    <Item>Item 8</Item>\n  </Carousel>\n  <br/>\n  <CarouselHooks>\n    <Item>Item 1</Item>\n    <Item>Item 2</Item>\n    <Item>Item 3</Item>\n    <Item>Item 4</Item>\n    <Item>Item 5</Item>\n    <Item>Item 6</Item>\n    <Item>Item 7</Item>\n    <Item>Item 8</Item>\n  </CarouselHooks>\n</div>\n\nexport default Index\n\nconst CarouselContainer = ({ children, sliding, direction }) => {\n  return <Fragment>\n    <div className=\"carousel_container\">\n      { children }\n    </div>\n    <style jsx>\n      {`\n        .carousel_container {\n          display: flex;\n          margin: 0 0 20px 20px;\n          transition: ${sliding ? \"none\" : \"transform 1s ease\"};\n          transform: ${!sliding ? 'translateX(calc(-80% - 20px))' : direction === 'prev' ? 'translateX(calc(2 * (-80% - 20px)))' : 'translateX(0%)'}\n        }\n      `}\n    </style>\n  </Fragment>\n}\n\nconst Wrapper = ({ children }) => {\n  return <Fragment>\n    <div className=\"wrapper\">\n      { children }\n    </div>\n    <style jsx>\n      {`\n        .wrapper {\n          width: 100%;\n          overflow: hidden;\n        }\n      `}\n    </style>\n  </Fragment>\n}\n\nconst CarouselSlot = ({ children, order }) => {\n  return <Fragment>\n    <div className=\"carousel_slot\">\n      { children }\n    </div>\n    <style jsx>\n      {`\n        .carousel_slot {\n          flex: 1 0 80%;\n          flex-basis: 80%;\n          margin-right: 20px;\n          order: ${order};\n        }\n      `}\n    </style>\n  </Fragment>\n}\n\nconst Item = ({ children }) => {\n  return <Fragment>\n    <div className=\"item\">\n      { children }\n    </div>\n    <style jsx>\n      {`\n        .item {\n          background: darkorange;\n          text-align: center;\n          padding: 50px;\n          color: white;\n          min-width: 160px;\n          margin: 0 16px 0 0;\n        }\n      `}\n    </style>\n  </Fragment>\n}\n"]},"metadata":{},"sourceType":"module"}