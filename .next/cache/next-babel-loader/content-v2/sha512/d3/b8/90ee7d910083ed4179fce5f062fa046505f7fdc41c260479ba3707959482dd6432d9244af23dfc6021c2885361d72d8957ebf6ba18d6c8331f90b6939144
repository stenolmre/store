}, 0) * 0.2;\n};\nexport var calcTotalWithoutTax = function calcTotalWithoutTax(arr, discount) {\n  return arr.map(function (el) {\n    return el.price * el.quantity;\n  }).reduce(function (acc, item) {\n    return acc + item;\n  }, 0) * 0.8;\n};\nexport var calcServerTotalPrice = function calcServerTotalPrice(arr, arrConditions, discount) {\n  return (arr.filter(function (product) {\n    return arrConditions.map(function (item) {\n      return item._id;\n    }).includes(product._id);\n  }).map(function (el) {\n    return el.price * arrConditions.filter(function (item) {\n      return item._id === el._id;\n    }).map(function (_item) {\n      return _item.quantity;\n    });\n  }).reduce(function (acc, item) {\n    return acc + item;\n  }, 0) * (1 - discount / 100)).toFixed(0);\n};\nexport var calcTotalPriceSuccessfulPayment = function calcTotalPriceSuccessfulPayment(arr, arrConditions, discount) {\n  return (arr.filter(function (product) {\n    return arrConditions.map(function (item) {\n      return item._id;\n    }).includes(product._id);\n  }).map(function (el) {\n    return el.price / 100 * arrConditions.filter(function (item) {\n      return item._id === el._id;\n    }).map(function (_item) {\n      return _item.quantity;\n    });\n  }).reduce(function (acc, item) {\n    return acc + item;\n  }, 0) * (1 - discount / 100)).toFixed(2);\n};","map":{"version":3,"sources":["/Users/stenolmre/Desktop/learn/utils/calculations.js"],"names":["React","calcTotalPrice","arr","map","el","price","quantity","reduce","acc","item","calcTotalPriceDiscount","discount","calcDiscount","calcTax","calcTotalWithoutTax","calcServerTotalPrice","arrConditions","filter","product","_id","includes","_item","toFixed","calcTotalPriceSuccessfulPayment"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB,C,CAEA;AACA;AACA;AACA;AACA;AACA;;AAEA,OAAO,IAAMC,cAAc,GAAG,SAAjBA,cAAiB,CAAAC,GAAG;AAAA,SAAIA,GAAG,CAACC,GAAJ,CAAQ,UAAAC,EAAE;AAAA,WAAIA,EAAE,CAACC,KAAH,GAAWD,EAAE,CAACE,QAAlB;AAAA,GAAV,EAAsCC,MAAtC,CAA6C,