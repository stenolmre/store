{"ast":null,"code":"import React from 'react'; // export const calcTotalPrice = (arr, arrConditions) => (\n//   arr\n//     .filter(product => arrConditions.map(item => item._id).includes(product._id))\n//     .map(el => el.price / 100 * arrConditions.filter(item => item._id === el._id).map(_item => _item.quantity))\n//     .reduce((acc, item) => acc + item, 0)\n// ).toFixed(2)\n\nexport var calcTotalPrice = function calcTotalPrice(arr) {\n  return arr.map(function (el) {\n    return el.price * el.quantity;\n  }).reduce(function (acc, item) {\n    return acc + item;\n  }, 0);\n}; // export const calcTotalPriceDiscount = (arr, arrConditions, discount) => (\n//   arr\n//     .filter(product => arrConditions.map(item => item._id).includes(product._id))\n//     .map(el => el.price / 100 * arrConditions.filter(item => item._id === el._id).map(_item => _item.quantity))\n//     .reduce((acc, item) => acc + item, 0) * (1 - (discount / 100))\n// ).toFixed(2)\n\nexport var calcTotalPriceDiscount = function calcTotalPriceDiscount(arr, discount) {\n  return arr.map(function (el) {\n    return el.price * el.quantity;\n  }).reduce(function (acc, item) {\n    return acc + item;\n  }, 0) * (1 - discount / 100);\n};\nexport var calcDiscount = function calcDiscount(arr, arrConditions, discount) {\n  return (arr.filter(function (product) {\n    return arrConditions.map(function (item) {\n      return item._id;\n    }).includes(product._id);\n  }).map(function (el) {\n    return el.price / 100 * arrConditions.filter(function (item) {\n      return item._id === el._id;\n    }).map(function (_item) {\n      return _item.quantity;\n    });\n  }).reduce(function (acc, item) {\n    return acc + item;\n  }, 0) * (discount / 100)).toFixed(2);\n};\nexport var calcServerTotalPrice = function calcServerTotalPrice(arr, arrConditions, discount) {\n  return (arr.filter(function (product) {\n    return arrConditions.map(function (item) {\n      return item._id;\n    }).includes(product._id);\n  }).map(function (el) {\n    return el.price * arrConditions.filter(function (item) {\n      return item._id === el._id;\n    }).map(function (_item) {\n      return _item.quantity;\n    });\n  }).reduce(function (acc, item) {\n    return acc + item;\n  }, 0) * (1 - discount / 100)).toFixed(0);\n};\nexport var calcTotalPriceSuccessfulPayment = function calcTotalPriceSuccessfulPayment(arr, arrConditions, discount) {\n  return (arr.filter(function (product) {\n    return arrConditions.map(function (item) {\n      return item._id;\n    }).includes(product._id);\n  }).map(function (el) {\n    return el.price / 100 * arrConditions.filter(function (item) {\n      return item._id === el._id;\n    }).map(function (_item) {\n      return _item.quantity;\n    });\n  }).reduce(function (acc, item) {\n    return acc + item;\n  }, 0) * (1 - discount / 100)).toFixed(2);\n};","map":{"version":3,"sources":["/Users/stenolmre/Desktop/learn/utils/calculations.js"],"names":["React","calcTotalPrice","arr","map","el","price","quantity","reduce","acc","item","calcTotalPriceDiscount","discount","calcDiscount","arrConditions","filter","product","_id","includes","_item","toFixed","calcServerTotalPrice","calcTotalPriceSuccessfulPayment"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB,C,CAEA;AACA;AACA;AACA;AACA;AACA;;AAEA,OAAO,IAAMC,cAAc,GAAG,SAAjBA,cAAiB,CAAAC,GAAG;AAAA,SAAIA,GAAG,CAACC,GAAJ,CAAQ,UAAAC,EAAE;AAAA,WAAIA,EAAE,CAACC,KAAH,GAAWD,EAAE,CAACE,QAAlB;AAAA,GAAV,EAAsCC,MAAtC,CAA6C,UAACC,GAAD,EAAMC,IAAN;AAAA,WAAeD,GAAG,GAAGC,IAArB;AAAA,GAA7C,EAAwE,CAAxE,CAAJ;AAAA,CAA1B,C,CAEP;AACA;AACA;AACA;AACA;AACA;;AAEA,OAAO,IAAMC,sBAAsB,GAAG,SAAzBA,sBAAyB,CAACR,GAAD,EAAMS,QAAN;AAAA,SAAmBT,GAAG,CAACC,GAAJ,CAAQ,UAAAC,EAAE;AAAA,WAAIA,EAAE,CAACC,KAAH,GAAWD,EAAE,CAACE,QAAlB;AAAA,GAAV,EAAsCC,MAAtC,CAA6C,UAACC,GAAD,EAAMC,IAAN;AAAA,WAAeD,GAAG,GAAGC,IAArB;AAAA,GAA7C,EAAwE,CAAxE,KAA8E,IAAKE,QAAQ,GAAG,GAA9F,CAAnB;AAAA,CAA/B;AAEP,OAAO,IAAMC,YAAY,GAAG,SAAfA,YAAe,CAACV,GAAD,EAAMW,aAAN,EAAqBF,QAArB;AAAA,SAAkC,CAC5DT,GAAG,CACAY,M